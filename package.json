{
  "name": "tridentframe",
  "productName": "TridentFrame",
  "version": "0.1.0-beta.12",
  "description": "Desktop animated images swiss-army knife",
  "author": {
    "name": "StahlFerro",
    "email": "forgeworkseven@gmail.com"
  },
  "license": "GPL-3.0-or-later",
  "homepage": "https://stahlferro.pages.dev/softwares/tridentframe",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StahlFerro/TridentFrame.git"
  },
  "bugs": {
    "url": "https://github.com/StahlFerro/TridentFrame/issues"
  },
  "main": "./main.js",
  "scripts": {
    "clean-build": "rimraf build/*",
    "clean-dist": "rimraf dist/*",
    "clean-cache": "rimraf temp/cache/*",
    "clean-previews": "rimraf temp/previews/*",
    "clean-engine": "rimraf engine/*",
    "clean-release": "rimraf release/*",
    "clean-mochawesome": "rimraf tests/javascript/mochawesome-report/*",
    "clean-all": "npm run clean-build && npm run clean-dist && npm run clean-engine && npm run clean-cache && npm run clean-previews",
    "css-build": "sass --no-source-map sass/bulmamods.scss css/bulmamods.css",
    "css-watch": "sass --no-source-map --watch sass/bulmamods.scss css/bulmamods.css",
    "dev": "cross-env DEPLOY_ENV=DEV electron .",
    "prod": "electron .",
    "prod-windows": "npm run wpbuild && npm run buildpy-windows && npm run prod",
    "prod-linux": "npm run wpbuild && npm run buildpy-linux && npm run prod",
    "liverelease": "npm run clean-dist && webpack --watch",
    "wpserve": "webpack serve --mode development --env NODE_ENV=DEV",
    "wpbuild": "npm run clean-dist && webpack --mode production --env NODE_ENV=PROD",
    "elbuild-windows": "npm run clean-release && electron-builder --config ./shipping-configs/electron-builder-win.json",
    "elbuild-linux": "npm run clean-release && electron-builder --config ./shipping-configs/electron-builder-linux.json",
    "buildpy-windows": "npm run clean-engine && pyinstaller tripy_windows64.spec",
    "buildpy-linux": "npm run clean-engine && pyinstaller tripy_linux.spec",
    "updatepy-windows": "npm run buildpy-windows && rimraf ./release/tridentframe/win-unpacked/resources/app/engine/ && xcopy engine release\\tridentframe\\win-unpacked\\resources\\app\\engine\\ /E",
    "updatepy-linux": "npm run buildpy-linux && rimraf ./release/tridentframe/linux-unpacked/resources/app/engine/ && cp -r ./engine/ ./release/tridentframe/linux-unpacked/resources/app/engine/",
    "release-windows": "npm run clean-build && npm run clean-cache && npm run clean-previews && npm run wpbuild && npm run buildpy-windows && npm run elbuild-windows && npm run scribe-version",
    "release-linux": "npm run clean-build && npm run clean-cache && npm run clean-previews && npm run wpbuild && npm run buildpy-linux && npm run elbuild-linux && npm run scribe-version",
    "scribe-version": "python writer/info_scribe.py",
    "test-py": "python -m pytest tests/python/unit/ tests/python/integration/ --html=tests/python/pytest-report/report.html --self-contained-html",
    "test-py-prod": "python -m pytest tests/python/production/ --html=tests/python/pytest-report/report.html --self-contained-html",
    "test-js-unit": "npm run clean-mochawesome && cross-env DEPLOY_ENV=DEV mocha './tests/javascript/unit/*.js' --require @babel/polyfill --require @babel/register --exit",
    "test-js-integration": "npm run clean-mochawesome && cross-env DEPLOY_ENV=DEV mocha './tests/javascript/integration/*.js' --require @babel/polyfill --require @babel/register --exit",
    "test-js": "npm run clean-mochawesome && cross-env DEPLOY_ENV=DEV mocha './tests/javascript/**/*.js' --require @babel/polyfill --require @babel/register --exit",
    "test-js-prod": "npm run clean-mochawesome && mocha './tests/javascript/**/*.js' --require @babel/polyfill --require @babel/register --exit",
    "test-all": "npm run test-py && npm run test-js && npm run test-py-prod && npm run test-js-prod"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "atomically": "^1.7.0",
    "concat-stream": "^2.0.0",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.18.10",
    "@babel/register": "^7.18.9",
    "@creativebulma/bulma-divider": "^1.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/free-regular-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@playwright/test": "^1.25.1",
    "@popperjs/core": "^2.11.6",
    "@vue/compiler-sfc": "^3.2.38",
    "babel-loader": "^8.2.5",
    "bulma": "^0.9.4",
    "bulma-checkradio": "^2.1.3",
    "bulma-slider": "^2.0.5",
    "chai": "^4.3.6",
    "click-outside-vue3": "^4.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "devtron": "^1.4.0",
    "electron": "^20.1.0",
    "electron-builder": "^23.3.3",
    "eslint": "^8.23.0",
    "eslint-plugin-vue": "^9.4.0",
    "file-loader": "^6.2.0",
    "highlight.js": "^11.6.0",
    "html-webpack-plugin": "^5.5.0",
    "json-minimizer-webpack-plugin": "^4.0.0",
    "make-fetch-happen": "^10.2.1",
    "mime-types": "^2.1.35",
    "mini-css-extract-plugin": "^2.6.1",
    "mitt": "^3.0.0",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "nanoid": "^4.0.0",
    "node-loader": "^2.0.0",
    "playwright": "^1.25.1",
    "python-shell": "^3.0.1",
    "regenerator-runtime": "^0.13.9",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.54.6",
    "sass-loader": "^13.0.2",
    "terser-webpack-plugin": "^5.3.6",
    "url-loader": "^4.1.1",
    "vue": "^3.2.38",
    "vue-click-outside": "^1.1.0",
    "vue-loader": "^17.0.0",
    "vue-slider-component": "^4.0.0-beta.4",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dashboard": "^3.3.7",
    "webpack-dev-server": "^4.10.1",
    "wtfnode": "^0.9.1"
  }
}
